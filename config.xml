<?xml version='1.0' encoding='utf-8'?>
<widget id="com.godlove.JAMBBiologyPAstQuestions" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>JAMBBiologyPastQuestions</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="godslovetech018@gmail.com" href="http://www.godslovetechnologies.com.ng/successapps/">
        Godslove Technologies
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-network-information" spec="D:\mobile\cordova plugins\cordova-plugin-network-information" />
    <plugin name="cordova-plugin-dialogs" spec="D:\mobile\cordova plugins\cordova-plugin-dialogs" />
    <engine name="android" spec="^7.0.0" />
</widget>
